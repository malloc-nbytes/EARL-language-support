{
        "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
        "name": "EARL",
        "patterns": [
                {
                        "include": "#keywords"
                },
                {
                        "include": "#strings"
                },
                {
                        "include": "#member_intrinsics"
                },
                {
                        "include": "#attributes"
                }
        ],
        "repository": {
                "keywords": {
                        "patterns": [
                                {
                                        "name": "keyword.control.earl",
                                        "match": "\\b(if|else|while|for|foreach|break|continue|return|match|some|none|print|println|warn|panic|exit|let|module|import|fn|len|open|when|class|constructor|this|in|__internal_move__|fprintln|fprint|enum|almost|full|int|float|tuple|list|bool|to|Char|CSV|IO|SinglyLL|List|Math|System|Encryption|Queue|Set|Stack|Str|Utils|type|typeof|Dict|const|true|false|option|file|unit|real|any|Matrix|sleep|datetime|env)\\b"
                                }
                        ]
                },
                "strings": {
                        "name": "string.quoted.double.earl",
                        "begin": "\"",
                        "end": "\"",
                        "patterns": [
                                {
                                        "name": "constant.character.escape.earl",
                                        "match": "\\\\."
                                }
                        ]
                },
                "member_intrinsics": {
                        "patterns": [
                                {
                                        "name": "support.function.earl",
                                        "match": "\\b\\w+\\.\\w+\\b"
                                }
                        ]
                },
                "attributes": {
                        "patterns": [
                                {
                                        "name": "variable.other.attribute.earl",
                                        "match": "(?<=@)\\w+\\b"
                                }
                        ]
                }
        },
        "scopeName": "source.earl"
}
